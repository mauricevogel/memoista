{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc --project tsconfig.json && tsc-alias -p tsconfig.json",
    "dev": "NODE_ENV=development nodemon -r tsconfig-paths/register src/app.ts",
    "start": "NODE_ENV=production node dist/src/app.js",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint src/**/*.ts test/**/*.ts --fix",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:e2e": "dotenv -e .env.test npm run test:e2e:ci",
    "test:e2e:ci": "NODE_ENV=test npm run prisma:migrate:deploy && jest --config ./jest-e2e.config.ts",
    "prisma:generate": "npx prisma generate",
    "prisma:studio": "npx prisma studio",
    "prisma:migrate:dev": "npx prisma migrate dev --create-only",
    "prisma:migrate:deploy": "npx prisma migrate deploy",
    "prisma:migrate:reset": "npx prisma migrate reset"
  },
  "keywords": [],
  "license": "UNLICENSED",
  "devDependencies": {
    "@faker-js/faker": "^8.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.6",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "nodemon": "^3.0.3",
    "prettier": "^3.2.4",
    "prisma": "^5.8.1",
    "supertest": "^6.3.4",
    "timekeeper": "^2.3.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@prisma/client": "^5.8.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "reflect-metadata": "^0.2.1",
    "routing-controllers": "^0.10.4",
    "typedi": "^0.10.0",
    "winston": "^3.11.0"
  }
}
